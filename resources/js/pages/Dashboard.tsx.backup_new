            (e) => (e.progress ?? 0) >= 100
        ).length;
        const pending = allEvaluations.filter(
            (e) => (e.progress ?? 0) < 100
        ).length;
        const avgProgress =
            allEvaluations.length > 0
                ? allEvaluations.reduce(
                      (sum, e) => sum + (e.progress ?? 0),
                      0
                  ) / allEvaluations.length
                : 0;

        return {
            total: allEvaluations.length,
            completed,
            pending,
            avgProgress: Math.round(avgProgress),
        };
    }, [evaluations]);

